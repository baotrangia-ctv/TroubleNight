import "Physics.fcc" as Physics
import "StdLibrary.fcc" as StdLib
import "EditorGenLib.fcc" as EditorLib

graph SafeBoxTrigger {
    event OnTriggerEnter(target entity<Entity>) {
        DispatchEventWithPlatform(OnPlayerNearSafeBox, target, PlatformType.Both, List<object>{target})
    }

    event OnTriggerLeave(target entity<Entity>) {
        DispatchEventWithPlatform(OnPlayerFarSafeBox, target, PlatformType.Both, List<object>{target})
    }
}