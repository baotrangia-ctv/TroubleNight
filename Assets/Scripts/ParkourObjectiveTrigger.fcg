import "Physics.fcc" as Physics
import "StdLibrary.fcc" as StdLib
import "EditorGenLib.fcc" as EditorLib

graph ParkourObjectiveTrigger {
    event OnTriggerEnter(target entity<Entity>) {
        var parentObject entity<Entity> = GetParent(thisEntity)
        var objectiveTag string = parentObject<Entity>.Tags[0]

        DispatchEventWithPlatform(OnPlayerGotObjective, target, PlatformType.Both, List<object>{target, objectiveTag})
    }
}